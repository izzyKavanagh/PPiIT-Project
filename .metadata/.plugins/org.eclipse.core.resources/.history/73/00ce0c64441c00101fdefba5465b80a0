public class VLANInterface {

    private String helperAddress;

    public void setHelperAddress(String helperAddress) {
        this.helperAddress = helperAddress;
    }

    @Override
    public String toString() {
        return "IP: " + ipAddress + ", Helper: " + helperAddress;
